{
  "name": "responsiveness",
  "version": "0.0.5",
  "description": "Responsive style tools for ReactJS",
  "keywords": [
    "react",
    "responsive"
  ],
  "license": "ISC",
  "homepage": "https://github.com/entria/responsiveness#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entria/responsiveness"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "gh-pages": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "scripts": {
    "build": "babel src -d lib",
    "start": "start-storybook -p 6006 -c storybook",
    "demo:build": "build-storybook -c storybook",
    "demo:gh-pages": "gh-pages -d storybook-static/",
    "demo:publish": "npm run demo:build && npm run demo:gh-pages",
    "prerelease": "npm run test && npm run build",
    "release:patch": "npm run prerelease && npm version patch && git push --follow-tags && npm publish",
    "release:minor": "npm run prerelease && npm version minor && git push --follow-tags && npm publish",
    "release:major": "npm run prerelease && npm version major && git push --follow-tags && npm publish"
  }
}
